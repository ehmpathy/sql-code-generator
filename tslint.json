{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-airbnb"],
  "rulesDirectory": ["tslint-consistent-codestyle"],
  "rules": {
    "no-console": {
      "severity": "warn"
    },
    "jsdoc-format": false,
    "import-name": false,
    "no-shadowed-variable": false,
    "object-literal-sort-keys": false,
    "no-empty": [true, "allow-empty-functions"],
    "interface-name": false,
    "ordered-imports": false,
    "max-line-length": false,
    "object-shorthand-properties-first": false,
    "no-this-assignment": false,
    "no-unused": true,
    "no-unnecessary-else": true,
    "no-unnecessary-type-annotation": false,
    "ter-arrow-parens": false,
    "member-ordering": false,
    "ter-indent": false,
    "array-type": false,
    "curly": false
  },
  "linterOptions": {
    "exclude": ["src/logic/__test_assets__/exampleProject/postgres/src/generated/fromSql/**/*"]
  }
}
